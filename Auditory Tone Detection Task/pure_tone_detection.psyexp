      <CodeComponent name="translation_code">
        <Param val="### START DEFINE MESSAGES TO PARTICIPANT ###&amp;#10;# messages are defined in order of appearance in&amp;#10;# the experiment&amp;#10;&amp;#10;# note that '\n' means 'newline', so use&amp;#10;# a \n every time you want the immediately following&amp;#10;# character to start on a new line&amp;#10;&amp;#10;WELCOME_TXT = (&amp;#10;    &quot;Welcome to the Tone Detection task\n\n&quot;&amp;#10;    &quot;Press SPACE to begin&quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT1 = (&amp;#10;    &quot;When you begin the task, you will be presented &quot;&amp;#10;    &quot;with two light bulbs on each side of the screen.&quot;&amp;#10;)&amp;#10;&amp;#10;CONTINUE_TXT = (&amp;#10;    &quot;PRESS SPACE to continue&quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT2 = (&amp;#10;    &quot;In each trial, the light bulbs will blink&quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT3 = (&amp;#10;    &quot;The left light bulb or 'light bulb 1' will always blink first&quot;&amp;#10;)&amp;#10;&amp;#10;CONTINUE_BULB1_TXT = (&amp;#10;    &quot;Press SPACE to see light bulb 1 blink&quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT4 = (&amp;#10;    &quot;After a brief pause, the second light bulb &quot; &amp;#10;    &quot;'light bulb 2' will also blink&quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT5 = (&amp;#10;    &quot;In each trial, ONE of the light bulbs will also make a noise&quot;&amp;#10;)&amp;#10;&amp;#10;CONTINUE_HEARSOUND_TXT = (&amp;#10;    &quot;press SPACEBAR to hear this sound&quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT6 = (&amp;#10;    &quot;Remember, although BOTH light bulbs will blink, &quot;&amp;#10;    &quot;only ONE light bulb will make a noise! &quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT7 = (&amp;#10;    &quot;It is your job to decide which light bulb made &quot;&amp;#10;    &quot;the noise by responding with the number keys.&quot;&amp;#10;)&amp;#10;&amp;#10;INSTRUCT_TXT8 = (&amp;#10;    &quot;IF you think that 'light bulb 1' made the noise, press '1'\n&quot;&amp;#10;    &quot;OR IF you think 'light bulb 2' made the noise, press '2'&quot;&amp;#10;)&amp;#10;&amp;#10;BEGIN_PRACTICE_TXT = (&amp;#10;    &quot;press 'p' to begin the practice trials!&quot;&amp;#10;)&amp;#10;&amp;#10;# message displayed during inter-stimulus interval&amp;#10;# (put empty string, &quot;&quot;, if you don't want any message&amp;#10;# to be displayed during ISI)&amp;#10;ISI_TXT = (&amp;#10;    &quot;PAUSE&quot;&amp;#10;)&amp;#10;&amp;#10;# feedback messages displayed depending on participant&amp;#10;# response&amp;#10;CORRECT_TXT = (&amp;#10;    &quot;Correct&quot;&amp;#10;)&amp;#10;INCORRECT_TXT = (&amp;#10;    &quot;Incorrect&quot;&amp;#10;)&amp;#10;&amp;#10;PROBE_TXT = (&amp;#10;    &quot;In which interval did you hear the tone?\n&quot;&amp;#10;    &quot;1 OR 2?&quot;&amp;#10;)&amp;#10;&amp;#10;PRACTICE_COMPLETE_TXT = (&amp;#10;    &quot;You have now completed the practice trials!\n&quot;&amp;#10;    &quot;If you understand the task, press 'c' to continue.\n&quot;&amp;#10;    &quot;If you do not understand the task, please ask for some help!&quot;&amp;#10;)&amp;#10;&amp;#10;BEGIN_TEST_TXT = (&amp;#10;    &quot;Upon pressing 'b', you will begin the testing session.\n\n&quot;&amp;#10;    &quot;Remember, in the testing session trials, the sound the &quot;&amp;#10;    &quot;light bulb makes will be shorter than the sounds made in the practice trials.\n\n&quot;&amp;#10;    &quot;The trials will also be a little faster, so stay alert!\n\n&quot;&amp;#10;    &quot;Press 'b' to begin&quot;&amp;#10;)&amp;#10;&amp;#10;END_TXT = (&amp;#10;    &quot;Thank you for completing the task!\n\n&quot;&amp;#10;    &quot;Press ‘e’ to exit&quot;&amp;#10;)&amp;#10;&amp;#10;### END DEFINE MESSAGES TO PARTICIPANT ###" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="WELCOME_TXT = &quot;V\u00e4lkommen till tonuppfattningstestet\n\nTryck SPACE f\u00f6r att b\u00f6rja&quot;;&amp;#10;INSTRUCT_TXT1 = &quot;N\u00e4r du p\u00e5b\u00f6rjar testet kommer du att visas tv\u00e5 lampor p\u00e5 varsin sida av sk\u00e4rmen.&quot;;&amp;#10;CONTINUE_TXT = &quot;Tryck SPACE f\u00f6r att forts\u00e4tta&quot;;&amp;#10;INSTRUCT_TXT2 = &quot;Under varje testomg\u00e5ng kommer lamporna att blinka&quot;;&amp;#10;INSTRUCT_TXT3 = &quot;Den v\u00e4nstra lampan, eller 'lampa 1', kommer alltid att blinka f\u00f6rst&quot;;&amp;#10;CONTINUE_BULB1_TXT = &quot;Tryck SPACE f\u00f6r att se lampa 1 blinka&quot;;&amp;#10;INSTRUCT_TXT4 = &quot;Efter en kort paus kommer den h\u00f6gra lampan att ocks\u00e5 blinka&quot;;&amp;#10;INSTRUCT_TXT5 = &quot;Under varje testomg\u00e5ng kommer EN av lamporna ocks\u00e5 att g\u00f6ra ett ljud&quot;;&amp;#10;CONTINUE_HEARSOUND_TXT = &quot;Tryck SPACE f\u00f6r att h\u00f6ra detta ljudet&quot;;&amp;#10;INSTRUCT_TXT6 = &quot;Kom ih\u00e5g att fast\u00e4n B\u00c5DA lamporna kommer att blinka, s\u00e5 kommer bara EN lampa att g\u00f6ra ett ljud!&quot;;&amp;#10;INSTRUCT_TXT7 = &quot;Det \u00e4r din uppgift att avg\u00f6ra vilken lampa som gjorde ljudet genom att svara med sifferknapparna.&quot;;&amp;#10;INSTRUCT_TXT8 = &quot;OM du tror att 'lampa 1' gjorde ljudet, tryck '1'\nELLER OM du tror att 'lampa 2' gjorde ljudet, tryck '2'&quot;;&amp;#10;BEGIN_PRACTICE_TXT = &quot;Tryck 'p' f\u00f6r att starta \u00f6vningsrundor&quot;;&amp;#10;ISI_TXT = &quot;PAUS&quot;;&amp;#10;CORRECT_TXT = &quot;R\u00e4tt&quot;;&amp;#10;INCORRECT_TXT = &quot;Fel&quot;;&amp;#10;PROBE_TXT = &quot;Under vilket intervall h\u00f6rde du tonen?\n1 ELLER 2?&quot;;&amp;#10;PRACTICE_COMPLETE_TXT = &quot;Du har nu avslutat \u00f6vningsrundorna!\nOm du f\u00f6rst\u00e5r uppgiften, tryck 'c' f\u00f6r att forts\u00e4tta.\nOm du inte f\u00f6rst\u00e5r uppgiften, v\u00e4nligen be om hj\u00e4lp!&quot;;&amp;#10;BEGIN_TEST_TXT = &quot;N\u00e4r du trycker p\u00e5 'b' s\u00e5 kommer testet att p\u00e5b\u00f6rjas.\n\nKom ih\u00e5g att under sj\u00e4lva testet s\u00e5 kommer ljudet som lamporna g\u00f6r att vara kortare \u00e4n ljuden i \u00f6vningsrundorna.\n\nRundorna kommer ocks\u00e5 att vara lite snabbare, s\u00e5 h\u00e5ll dig p\u00e5 alerten!\n\nTryck 'b' f\u00f6r att b\u00f6rja&quot;;&amp;#10;END_TXT = &quot;Tack f\u00f6r att du slutf\u00f6rde testet!\n\nTryck 'e' f\u00f6r att avsluta.&quot;;&amp;#10;" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin Experiment"/>
