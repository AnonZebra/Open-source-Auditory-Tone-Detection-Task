        <Param val="# number of trials to run &amp;#10;NUM_TRIALS = 50&amp;#10;&amp;#10;#Determine whether instructions should be run&amp;#10;if expInfo['Instructions'] == 'Yes':&amp;#10;    instructionsToggle = 1&amp;#10;if expInfo['Instructions'] == 'No':&amp;#10;    instructionsToggle = 0&amp;#10;# OVERRIDE (specific for experiment runs in Stockholm,&amp;#10;# for Janina Neufeld's synesthesia project):&amp;#10;# _always_ skip instructions if non-'Quiet' version of&amp;#10;# experiment (condition) is run&amp;#10;if expInfo['Condition'] != 'Quiet':&amp;#10;    instructionsToggle = 0&amp;#10;&amp;#10;&amp;#10;#Determine whether practice should be run&amp;#10;if expInfo['Practice'] == 'Yes':&amp;#10;    practiceToggle = 1&amp;#10;if expInfo['Practice'] == 'No':&amp;#10;        practiceToggle = 0&amp;#10;&amp;#10;#Create calibration variable based on what was input in initial GUI expInfo['Calibration']&amp;#10;calibration = float(expInfo['Calibration'])&amp;#10;&amp;#10;#Set noise tone onset times based on condition selected in initial GUI (or 'expInfo['Condition'])&amp;#10;if expInfo['Condition'] == 'Simultaneous': #if Condition is 'Simultaneous', make onset 200ms into the trial &amp;#10;    toneOnset = 0.22 #.22 rather than .20 to account account for the 20ms gap that exists before the noise (starting at 20ms into the trial) at the start of the trial&amp;#10;elif expInfo['Condition'] == 'Backward':  #else if Condition is 'Backward' : make Onset start at 0 (20ms before noise) &amp;#10;    toneOnset = 0 &amp;#10;else: #else, just start the tone in sync with the start of the noise (as per the 'Quiet' Condition)&amp;#10;    toneOnset = 0.02 &amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="NUM_TRIALS = 50;&amp;#10;if ((expInfo[&quot;Instructions&quot;] === &quot;Yes&quot;)) {&amp;#10;    instructionsToggle = 1;&amp;#10;}&amp;#10;if ((expInfo[&quot;Instructions&quot;] === &quot;No&quot;)) {&amp;#10;    instructionsToggle = 0;&amp;#10;}&amp;#10;if ((expInfo[&quot;Condition&quot;] !== &quot;Quiet&quot;)) {&amp;#10;    instructionsToggle = 0;&amp;#10;}&amp;#10;if ((expInfo[&quot;Practice&quot;] === &quot;Yes&quot;)) {&amp;#10;    practiceToggle = 1;&amp;#10;}&amp;#10;if ((expInfo[&quot;Practice&quot;] === &quot;No&quot;)) {&amp;#10;    practiceToggle = 0;&amp;#10;}&amp;#10;calibration = Number.parseFloat(expInfo[&quot;Calibration&quot;]);&amp;#10;if ((expInfo[&quot;Condition&quot;] === &quot;Simultaneous&quot;)) {&amp;#10;    toneOnset = 0.22;&amp;#10;} else {&amp;#10;    if ((expInfo[&quot;Condition&quot;] === &quot;Backward&quot;)) {&amp;#10;        toneOnset = 0;&amp;#10;    } else {&amp;#10;        toneOnset = 0.02;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
